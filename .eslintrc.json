{
	"extends": ["./node_modules/@buildinams/lint/eslint/react-javascript"],
	"rules": {
		"jsx-a11y/label-has-associated-control": ["warn"],
		"jsx-a11y/no-autofocus": ["warn"],
		"react/prop-types": ["warn", {
			"ignore": ["children"]
		}],
		"no-unused-vars": ["warn"],
		"no-console": ["warn"]
	},
	// Ignore console warning for specific directories
	"overrides": [
		{
			"files": ["*.example.js", "server/**/*.js", "scripts/**/*.js", "src/analytics/**/*.js"],
			"rules": {
				"no-console": ["off"]
			}
		}
	]
}
